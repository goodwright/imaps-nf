{
    "inputs": [
        {
            "name": "fastq",
            "label": "FASTQ File",
            "type": "data",
            "dataType": "fastq",
            "required": true
        }
    ],
    "outputs": [
        {
            "name": "fastq",
            "type": "file",
            "match": "*.fastq*"
        },
        {
            "name": "report",
            "type": "file",
            "match": "*.html"
        },
        {
            "name": "data",
            "type": "file",
            "match": "*.zip"
        },
        {
            "name": "fastqc_version",
            "type": "basic",
            "match": "fastqc_version:"
        },
        {
            "name": "quality_pass",
            "type": "basic",
            "match": "quality_pass:"
        }
    ]
}